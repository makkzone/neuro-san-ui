{
    "name": "nextfront",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "about:dev": "echo 'Starts the UI in dev mode on port 3000 on the local system'",
        "dev": "next dev --hostname 0.0.0.0 --port 3000",
        "about:lint": "echo 'Checks for issues in the Typescript and Javascript code using eslint'",
        "lint": "next lint",
        "about:lint-summary": "echo 'Generates a summary of number of issues per ESLint rule'",
        "lint-summary": "next lint -f summary",
        "about:lint-rules": "Outputs JSON showing which ESLint rules are enabled and which are disabled",
        "lint-rules": "npx eslint --print-config jest.config.ts",
        "about:build": "echo 'Creates a production build of the UI project using `next build`'",
        "build": "next build",
        "about:start": "echo 'Starts the UI in production mode. Requires `build` to have been run beforehand.'",
        "start": "next start",
        "about:unit_test": "echo 'Runs the front-end unit __tests__ using `jest`'",
        "unit_test": "jest '__tests__/component' '__tests__/pages' '__tests__/unit' --config ./jest_unit.config.ts",
        "about:integration_test": "echo 'Runs the front-end unit tests using `jest`'",
        "integration_test": "jest '__tests__/integration' --config ./jest_integration.config.ts",
        "about:check-format": "Runs prettier to check if the code is formatted correctly",
        "commit-check": "yarn run tsc ; yarn run ts-prune --error ; yarn run check-format --write ; ../build_scripts/run_eslint.sh ; yarn run unit_test ; yarn run depcheck",
        "check-format": "prettier --check .",
        "generate": "cp -r ../proto . && ./grpc/do_typescript_generate.sh"
    },
