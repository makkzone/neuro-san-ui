{
    "name": "@cognizant-ai-lab/neuro-san-ui",
    "version": "1.1.1",
    "private": true,
    "license": "SEE LICENSE IN LICENSE.txt",
    "type": "module",
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build:app": "tbd",
        "build:lib": "tbd: yarn  clean && yarn  generate &&  tsc --project packages/ui-common/tsconfig.build.json && cp apps/main/styles/globals.css dist/styles && yarn  fix-esm-import-path dist",
        "check-format": "tbd: prettier --check .",
        "clean": "rm -rf dist generated",
        "commit-check": "yarn  tsc ; yarn  ts-pe --error ; yarn  depcheck ; yarn  check-format --write ; ./build_scripts/run_eslint.sh ; yarn  unit_test",
        "dev": "yarn workspace @cognizant-ai-lab/neuro-san-ui-app run dev",
        "generate": "rm -rf packages/ui-common/generated && cd packages/ui-common && ./build_scripts/do_openapi_generate.sh",
        "lint": "tbd",
        "lint-rules": "tbd",
        "lint-summary": "tbd",
        "publish": "yarn workspace @cognizant-ai-lab/ui-common npm publish",
        "start": "tbd",
        "unit_test": "tbd: jest  'packages/ui-common/__tests__/components' 'apps/main/__tests__/pages' 'packages/ui-common/__tests__/unit' --config ./jest_unit.config.ts"
    },
    "packageManager": "yarn@4.9.4",
    "devDependencies": {
        "@babel/core": "7.23.7",
        "@babel/preset-env": "7.28.3",
        "@eslint/compat": "1.3.2",
        "@eslint/eslintrc": "3.3.1",
        "@eslint/js": "9.32.0",
        "@jest/types": "30.0.5",
        "@next/eslint-plugin-next": "15.4.6",
        "@testing-library/dom": "10.4.1",
        "@testing-library/jest-dom": "6.6.4",
        "@types/babel__core": "7.20.5",
        "@types/dagre": "0.7.53",
        "@types/debug": "4.1.12",
        "@types/eslint": "9.6.1",
        "@types/jest": "30.0.0",
        "@types/lodash": "4.17.12",
        "@types/lodash-es": "4.17.12",
        "@types/node": "22.17.1",
        "@types/prop-types": "15.7.11",
        "@types/react": "18.2.0",
        "@types/react-dom": "18.2.18",
        "@typescript-eslint/eslint-plugin": "8.39.0",
        "@typescript-eslint/parser": "8.39.0",
        "babel-jest": "30.1.1",
        "depcheck": "1.4.7",
        "eslint": "9.32.0",
        "eslint-config-next": "15.4.6",
        "eslint-config-prettier": "10.1.8",
        "eslint-formatter-summary": "2.0.2",
        "eslint-plugin-import": "2.32.0",
        "eslint-plugin-jest": "29.0.1",
        "eslint-plugin-jest-dom": "5.5.0",
        "eslint-plugin-react": "7.37.5",
        "eslint-plugin-react-hooks": "5.2.0",
        "eslint-plugin-testing-library": "7.6.3",
        "eslint-plugin-unicorn": "60.0.0",
        "fix-esm-import-path": "1.10.3",
        "globals": "16.3.0",
        "jest": "30.0.5",
        "jest-environment-jsdom": "30.0.5",
        "jest-fail-on-console": "3.3.1",
        "prettier": "3.6.2",
        "react": "18.2.0",
        "ts-jest": "29.4.1",
        "ts-node": "10.9.2",
        "ts-prune": "0.10.3",
        "typescript": "5.9.2"
    }
}
