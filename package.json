{
  "name": "nextfront",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "about:dev": "echo 'Starts the UI in dev mode on port 3000 on the local system'",
    "dev": "next dev --hostname 0.0.0.0 --port 3000",
    "about:lint": "echo 'Checks for issues in the Typescript and Javascript code using eslint'",
    "lint": "next lint",
    "about:lint-summary": "echo 'Generates a summary of number of issues per ESLint rule'",
    "lint-summary": "next lint -f summary",
    "about:build": "echo 'Creates a production build of the UI project using `next build`'",
    "build": "next build",
    "about:start": "echo 'Starts the UI in production mode. Requires `build` to have been run beforehand.'",
    "start": "next start",
    "about:unit_test": "echo 'Runs the front-end unit __tests__ using `jest`'",
    "unit_test": "jest '__tests__/unit' --verbose --config jest.config.ts",
    "about:component_test": "echo 'Runs the front-end component-based tests using `jest` and `react-testing-library`'",
    "component_test": "jest '__tests__/component' --verbose --config jest.config.mjs",
    "about:check-format": "Runs Prettier to check if the code is formatted correctly",
    "check-format": "prettier --check ./**/*.{js,jsx,ts,tsx,json,html,css,md}"
  },
