{
    "name": "@cognizant-ai-lab/neuro-san-ui",
    "version": "1.1.0",
    "private": true,
    "description": "The root package for the Neuro-SAN UI monorepo, containing the MAA UI web app and common components.",
    "license": "SEE LICENSE IN LICENSE.txt",
    "type": "module",
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build:app": "tbd",
        "build:lib": "yarn workspace @cognizant-ai-lab/ui-common build",
        "check-format": "yarn prettier --check .",
        "clean": "rm -rf dist generated",
        "commit-check": "yarn tsc; yarn knip; yarn check-format; yarn lint; yarn test",
        "dev": "yarn workspace @cognizant-ai-lab/neuro-san-web run dev",
        "generate": "rm -rf packages/ui-common/generated && cd packages/ui-common && ./build_scripts/do_openapi_generate.sh",
        "knip": "knip --no-config-hints",
        "lint": "eslint",
        "lint-rules": "tbd",
        "lint-summary": "tbd",
        "publish": "yarn workspace @cognizant-ai-lab/ui-common npm publish",
        "start": "tbd",
        "test": "jest --config ./jest_unit.config.ts",
        "tsc": "tsc --project tsconfig.json"
    },
    "packageManager": "yarn@4.9.4",
    "devDependencies": {
        "@babel/core": "7.23.7",
        "@babel/preset-env": "7.28.3",
        "@emotion/react": "11.13.3",
        "@emotion/styled": "11.13.0",
        "@eslint/compat": "1.3.2",
        "@eslint/eslintrc": "3.3.1",
        "@eslint/js": "9.32.0",
        "@jest/types": "30.0.5",
        "@mui/icons-material": "7.3.1",
        "@mui/material": "7.3.1",
        "@next/eslint-plugin-next": "15.4.6",
        "@testing-library/dom": "10.4.1",
        "@testing-library/jest-dom": "6.6.4",
        "@testing-library/react": "16.3.0",
        "@testing-library/user-event": "14.6.1",
        "@types/babel__core": "7.20.5",
        "@types/eslint": "9.6.1",
        "@types/jest": "30.0.0",
        "@types/lodash": "4.17.12",
        "@types/lodash-es": "4.17.12",
        "@types/node": "22.17.1",
        "@types/react": "18.2.0",
        "@types/react-dom": "18.2.0",
        "@typescript-eslint/eslint-plugin": "8.39.0",
        "@typescript-eslint/parser": "8.39.0",
        "babel-jest": "30.1.1",
        "eslint": "9.32.0",
        "eslint-config-next": "15.4.6",
        "eslint-config-prettier": "10.1.8",
        "eslint-formatter-summary": "2.0.2",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jest": "29.0.1",
        "eslint-plugin-jest-dom": "5.5.0",
        "eslint-plugin-react": "7.37.5",
        "eslint-plugin-react-hooks": "5.2.0",
        "eslint-plugin-testing-library": "7.6.3",
        "eslint-plugin-unicorn": "60.0.0",
        "globals": "16.3.0",
        "jest": "30.0.5",
        "jest-environment-jsdom": "30.0.5",
        "jest-fail-on-console": "3.3.1",
        "knip": "5.63.1",
        "prettier": "3.6.2",
        "react": "18.2.0",
        "ts-jest": "29.4.1",
        "ts-node": "10.9.2",
        "typescript": "5.9.2"
    },
    "resolutions": {
        "@types/react": "18.2.0",
        "@types/react-dom": "18.2.0"
    }
}
